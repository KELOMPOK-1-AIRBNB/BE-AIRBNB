                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "description": "Username user",
                                        "example": "user123"
                                    },
                                    "password": {
                                        "type": "string",
                                        "description": "passwword user",
